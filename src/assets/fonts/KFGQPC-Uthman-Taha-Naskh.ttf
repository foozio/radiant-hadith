<script type="text/javascript">
  function update_preview()
  {
    const MAX_SIZE = 100;
    const MIN_SIZE = 20;

    var preview_text = $("#preview-text").val();
    var font_size = parseInt($("#fontsize").text());

    var reset_font_size = false;

    if(!Number.isInteger(font_size)){
      font_size = 40;
      reset_font_size = true;
    }
    else if( font_size < MIN_SIZE || font_size > MAX_SIZE )
    {
      font_size = Math.min(MAX_SIZE , Math.max(MIN_SIZE, font_size));
      reset_font_size = true;
    }

    if(reset_font_size) {
      $("#fontsize").html(font_size);
    }

    var data_params = {
      text: preview_text,
      fontsize: font_size
    };

    if (typeof(Storage) !== "undefined") {
      localStorage.setItem("preview-text", preview_text);
      localStorage.setItem("fontsize", font_size);
    }
    var data_str = jQuery.param(data_params);

    $(".font-preview").each(function( index ) {
      var data_src = $(this).attr('data-simage');

      if(font_size != 40 || preview_text !== "") {
        var data_src = $(this).attr('data-dimage') + "?" + data_str;
      }
      $(this).attr('src',data_src);
    });
  }

  function reset_fontpreview_values()
  {
    $("#preview-text").val("");
    $("#fontsize").html("40");
    update_preview();
  }

  $('#preview-text').on('keyup keydown change',function(){
    update_preview();
  });

  $('#fontsizedecrease').click(function(){
    var font_size = parseInt($("#fontsize").text());
    $("#fontsize").html(font_size-5);
    update_preview();
  });

  $('#fontsizeincrease').click(function(){
    var font_size = parseInt($("#fontsize").text());
    $("#fontsize").html(font_size+5);
    update_preview();
  });

  $(document).ready(function () {
    if (typeof(Storage) !== "undefined") {
      var preview_text = localStorage.getItem("preview-text");
      var font_size = localStorage.getItem("fontsize");

      if(preview_text !== null && font_size !== null)
      {
        // Set the values
        $("#preview-text").val(preview_text);
        $("#fontsize").html(font_size);
        update_preview();
      }
    }
    else {
      reset_fontpreview_values();
    }

    $("#preview-reset").click(function(e){
      reset_fontpreview_values();
      e.preventDefault();
    });
  });
</script>



